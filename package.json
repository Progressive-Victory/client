{
  "name": "@progressive-victory/client",
  "version": "0.3.1",
  "description": "Modified Discord.js client for discord bots",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": "https://github.com/Progressive-Victory/discord-client",
  "author": "Okami",
  "contributors": [
    "John William Bowen <bowenjw22@outlook.com>"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "build": "rimraf ./dist && swc ./src --out-dir dist && tsc --emitDeclarationOnly",
    "prestart": "yarn build",
    "start": "node ./dist/index.js",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "postinstall": "swc ./src --out-dir dist && tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "discord.js": "^14.13.0",
    "logger": "Progressive-Victory/Logger"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
