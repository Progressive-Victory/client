{
  "name": "discord-client",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/Progressive-Victory/discord-client",
  "author": "Okami",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "build": "rimraf ./dist && swc ./src --out-dir dist",
    "run": "node ./dist/index.js",
    "start": "yarn build && yarn run run",
    "prepare": "yarn build",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.93",
    "logger": "Progressive-Victory/Logger"
  }
}
